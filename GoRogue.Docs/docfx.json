{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "GoRogue/**.csproj"
          ],
		  "exclude": [
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
	  "properties": {
		  "TargetFramework": "net5"
	  }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
	"xref": [
      "https://thesadrogue.github.io/TheSadRogue.Primitives/xrefmap.yml"
    ],
	"xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
  }
}